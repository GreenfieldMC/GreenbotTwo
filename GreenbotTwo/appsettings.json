{
  "CommandSettings": {
    "InstallCommand": {
      "WindowsInstallLink": "https://www.youtube.com/watch?v=b3aSSvot3s8",
      "MacInstallLink": "https://www.youtube.com/watch?v=kcFs8G1VBcE"
    },
    "BetapackCommand": {
      "DefaultBranch": "master"
    }
  },
  "ApiEndpoints": {
    "MojangApi": {
      "BaseAddress": "https://api.mojang.com/"
    }
  },
  "BuilderApplicationSettings": {
    "MinimumNumberOfHouseImages": 2,
    "MaximumNumberOfHouseImages": 6,
    "MinimumNumberOfOtherImages": 0,
    "MaximumNumberOfOtherImages": 4,
    "MaximumWhyJoinLength": 1200,
    "MaximumAdditionalBuildingInfoLength": 1200,
    "MaximumAdditionalCommentsLength": 500,
    "MaximumPerFileSizeBytes": 10485760
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Expressions" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/error-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "@l in ['Trace', 'Verbose', 'Debug', 'Information', 'Warning']"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "Logs/verbose-.txt",
                  "rollingInterval": "Day",
                  "retainedFileCountLimit": 14,
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
              }
            ]
          }
        }
      }
    ],
    "Properties": {
      "Application": "GreenbotTwo"
    }
  }
}